{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "requireConfigFile": false,
        "babelOptions": {
            "plugins": [
                "@babel/plugin-proposal-class-properties"
            ]
        }
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
